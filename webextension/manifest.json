{
  "description": "Native messaging example add-on",
  "manifest_version": 2,
  "name": "Ouinet",
  "version": "1.0",
  "icons": {
    "48": "todo.png"
  },

  "applications": {
    "gecko": {
      "id": "peter@equalit.ie",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "battery.svg",
    "browser_style": true
  },
  
  "permissions": ["proxy", "<all_urls>", "nativeMessaging"]
}
